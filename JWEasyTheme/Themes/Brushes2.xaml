<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush
        x:Key="SystemBaseHighColorBrush1"
        Color="{ThemeResource SystemBaseHighColor}" />
    <SolidColorBrush
        x:Key="SystemBaseLowColorBrush1"
        Color="{ThemeResource SystemBaseLowColor}" />
    <SolidColorBrush
        x:Key="SystemAltHighColorBrush1"
        Color="{ThemeResource SystemAltHighColor}" />
    <SolidColorBrush
        x:Key="SystemAltLowColorBrush1"
        Color="{ThemeResource SystemAltLowColor}" />
    <SolidColorBrush
        x:Key="SystemAltMediumColorBrush1"
        Color="{ThemeResource SystemAltMediumColor}" />
    <SolidColorBrush
        x:Key="SystemAltMediumHighColorBrush1"
        Color="{ThemeResource SystemAltMediumHighColor}" />
    
</ResourceDictionary>
